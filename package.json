{
  "name": "testapp",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "node server.mjs",
    "dev": "concurrently \"npm run server\" \"vite\"",
    "server": "node server.mjs",
    "build": "tsc -b && vite build",
    "clean": "rm -rf dist",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "duplicate-check": "jscpd --min-lines 10 --min-tokens 50 --ignore 'node_modules/**,dist/**,coverage/**,**/*.test.*' --reporters consoleFull .",
    "preflight": "npm run lint && npm run duplicate-check && npm run build && npm run test:coverage",
    "migrate": "node tools/scripts/migrate.js",
    "rollback": "node tools/scripts/rollback.js",
    "test:db": "node tools/scripts/test-db.js",
    "cli": "node cli/index.js",
    "prepare": "husky"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@octokit/app": "^14.0.0",
    "@octokit/webhooks": "^12.1.3",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "i18next": "^25.2.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.344.0",
    "mime": "^4.0.0",
    "pg": "^8.16.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.5.3",
    "react-router-dom": "^6.30.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^4.0.8",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.2.0",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "gh-pages": "^6.3.0",
    "globals": "^15.9.0",
    "husky": "^9.1.7",
    "jscpd": "^4.0.5",
    "jsdom": "^27.1.0",
    "lint-staged": "^15.2.11",
    "postcss": "^8.4.35",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.4.1",
    "terser": "^5.43.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^7.2.2",
    "vitest": "^4.0.8",
    "yaml-lint": "^1.7.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "npx prettier --write"
    ],
    "*.yml": [
      "npx yaml-lint"
    ]
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2
  }
}
